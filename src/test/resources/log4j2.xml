<?xml version="1.0" encoding="UTF-8"?>
<configuration status="OFF">

    <!-- 控制台输出 -->
    <appenders>
        <!-- 控制台输出 -->
        <Console name="consoleLog" target="SYSTEM_OUT">
            <PatternLayout
                    pattern="[%p] %d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
    </appenders>

    <loggers>
        <root level="DEBUG">
            <appender-ref ref="consoleLog"/>
        </root>
    </loggers>

</configuration>